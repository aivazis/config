#
# Identify this build target
#
    TARGET_ID = Darwin-8.x_x86
#
# Pick an operating system and compilers
#
    TARGET_PLATFORM = Darwin-8.x
    TARGET_COMPILER = gcc-4.x-Darwin
#
# The list of compilers to use
#
    TARGET_CC = $(TARGET_COMPILER)
    TARGET_CXX = $(TARGET_COMPILER)
    TARGET_F77 = $(TARGET_COMPILER)
    TARGET_NVCC = nvcc
#
# Here are the build-specific flags
#

#
# C
#
    TARGET_CC_FLAGS =
    TARGET_CC_DEFINES =
    TARGET_CC_INCLUDES = $(DIR_TARGET)/$(TARGET_ID)
    TARGET_LCC_FLAGS =
    TARGET_LCC_LIBPATH =
#
# cuda
#
    TARGET_NVCC_FLAGS =
    TARGET_NVCC_DEFINES =
    TARGET_NVCC_INCLUDES = $(DIR_TARGET)/$(TARGET_ID)
    TARGET_LNVCC_FLAGS =
    TARGET_LNVCC_LIBPATH =
#
# C++
#
    TARGET_CXX_FLAGS =
    TARGET_CXX_DEFINES =
    TARGET_CXX_INCLUDES = $(DIR_TARGET)/$(TARGET_ID)
    TARGET_LCXX_FLAGS =
    TARGET_LCXX_LIBPATH =
#
# FORTRAN
#
    TARGET_F77_FLAGS =
    TARGET_F77_DEFINES =
    TARGET_F77_INCLUDES = $(DIR_TARGET)/$(TARGET_ID)
    TARGET_LF77_FLAGS =
    TARGET_LF77_LIBPATH =

